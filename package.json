{
  "name": "express-debug-request",
  "version": "0.3.2",
  "description": "Express dummy server to test requests",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/node-tydom-client",
  "license": "MIT",
  "scripts": {
    "start": "DEBUG=express-debug-request FORWARD_ babel-node --extensions .js,.ts src/index.ts",
    "start:watch": "nodemon --ext \"ts,tsx\" --watch \"src/\" --exec \"yarn start\"",
    "test": "yarn pretty && yarn lint && yarn spec",
    "spec": "DEBUG=express-debug-request CI=true jest --runInBand --colors",
    "spec:watch": "yarn spec --watch",
    "spec:coverage": "yarn spec --coverage",
    "lint": "eslint --ext .ts src/",
    "pretty": "prettier --check '{src,test}/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "build": "yarn build:source && yarn build:typings && cp -rf src/assets lib && rsync -avh --remove-source-files lib/src/* lib/ && rm -rf lib/src",
    "build:source": "babel --source-maps --extensions .js,.ts --out-dir lib/ --delete-dir-on-start --ignore **/__tests__ src/",
    "build:typings": "tsc --declaration",
    "build:watch": "yarn build:source --watch --verbose",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "abort-controller": "^3.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "got": "^10.7.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.4",
    "@types/jest": "^25.1.5",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@types/source-map-support": "^0.5.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-plugin-module-name-mapper": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.2.7",
    "nock": "^12.0.3",
    "prettier": "^2.0.2",
    "supertest": "^4.0.2",
    "typescript": "^3.8.3"
  }
}
